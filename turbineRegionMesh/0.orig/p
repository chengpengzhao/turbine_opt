FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField      uniform 0;

boundaryField
{
    blade1
    {
        type            zeroGradient;
    }
    blade2
    {
        type            zeroGradient;
    }
    blade3
    {
        type            zeroGradient;
    }
    AMI_in
    {
        type            cyclicAMI;
        value   	$internalField;
    }
    AMI_out
    {
        type            cyclicAMI;
        value   	$internalField;
    }
    inlet
    {
        type            freestreamPressure;
        freestreamValue $internalField;
    }
    outlet
    {
        type            freestreamPressure;
        freestreamValue $internalField;
    }
    walls
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
}

// ************************************************************************* //
