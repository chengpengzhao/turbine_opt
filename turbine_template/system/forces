forcesBlade1
{

    type            forces;
    libs            ("libforces.so");
    patches         (blade1);
    outputControl timeStep;
    outputInterval 1;
    rho     rhoInf;
    log         true;
    rhoInf      1e3;
    pName p;
    UName U;
    CofR (0 0 0);
}
